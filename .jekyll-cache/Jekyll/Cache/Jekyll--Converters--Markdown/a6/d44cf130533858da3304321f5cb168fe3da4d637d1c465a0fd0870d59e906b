    #region 7.2 è‡ªå·±å®ç°ä¸€ä¸ª æ ¹æ®æ¡ä»¶æŸ¥æ‰¾çš„ æ–¹æ³• - Predicate - è¿”å›bool
    ///
    /// è‡ªå·±å®ç°ä¸€ä¸ª æ ¹æ®æ¡ä»¶æŸ¥æ‰¾çš„ æ–¹æ³•
    ///
    static List MyFindAll(this List list, Predicate predicate)
    {
        //åˆ›å»ºæ–°é›†åˆ
        List newList = new List();
        //éå†è€é›†åˆ
        foreach (T item in list)
        {
            //å¦‚æœè€é›†åˆé‡Œçš„ item ç¬¦åˆ æ¡ä»¶ï¼Œåˆ™ æŠŠitem æ·»åŠ åˆ° æ–°é›†åˆä¸­
            //è°ƒç”¨ä¼ å…¥çš„ predicateå§”æ‰˜ æ–¹æ³•ï¼Œä½œä¸º æ˜¯å¦ç¬¦åˆæ¡ä»¶çš„åˆ¤æ–­
            if (predicate(item))
            {
                newList.Add(item);
            }
        }
        return newList; 
    } 
    #endregion

    #region 7.2 è‡ªå·±å®ç°ä¸€ä¸ª æ ¹æ®æ¡ä»¶åˆ é™¤çš„ æ–¹æ³• - Predicate - è¿”å›boolå€¼çš„å§”æ‰˜
    ///
    /// 7.2 è‡ªå·±å®ç°ä¸€ä¸ª æ ¹æ®æ¡ä»¶åˆ é™¤çš„ æ–¹æ³• - Predicate - è¿”å›boolå€¼çš„å§”æ‰˜
    ///
    static void DeleteItem(this List list, Predicate deleteWhere)
    {
        //è¦åˆ é™¤çš„å…ƒç´ é›†åˆ
        List deletedItems = new List();
        //éå† æ•°æ®é›†åˆ æ ¹æ®æ¡ä»¶æ‰¾åˆ°è¦åˆ é™¤çš„å…ƒç´  æ·»åŠ åˆ° åˆ é™¤å…ƒç´ é›†åˆä¸­
        foreach (T item in list)
        {
            if (deleteWhere(item))//æ ¹æ® ä¼ å…¥çš„ æ¡ä»¶ æ‰¾åˆ°è¦åˆ é™¤çš„å…ƒç´ 
            {
                deletedItems.Add(item);
            }
        }
        //éå†è¦åˆ é™¤çš„å…ƒç´ ï¼Œåœ¨æ•°æ®é›†åˆä¸­åˆ é™¤
        foreach (T delItem in deletedItems)
        {
            list.Remove(delItem);
        }
    } 
    #endregion

    #region 7.3 ç³»ç»Ÿæ³›å‹å§”æ‰˜ - Comparison - è¿”å› int
    ///
    /// 7.3 ç³»ç»Ÿæ³›å‹å§”æ‰˜ - Comparison - è¿”å› int
    ///
    static void TestComparison()
    {
        List list = GetDogList();
        list.Sort(delegate(C01Dog x, C01Dog y)
        {
            return y.Age - x.Age;
        });

        list.ForEach(delegate(C01Dog d)
        {
            Console.WriteLine(d.Name + "," + d.Age);
        });
    } 
    #endregion

    #region 7.4 ç³»ç»Ÿæ³›å‹å§”æ‰˜ - Func - è‡ªå®šä¹‰è¿”å›å€¼ç±»å‹
    ///
    /// 7.4 ç³»ç»Ÿæ³›å‹å§”æ‰˜ - Func - è‡ªå®šä¹‰è¿”å›å€¼ç±»å‹
    ///
    static void TestFunc()
    {
        List list = GetDogList();
        //ä¸šåŠ¡ï¼šå®Œæ•´å†™æ³•ï¼šå°† dogé›†åˆ è½¬æˆ smallDogé›†åˆ

        //7.4.1 ä½¿ç”¨é›†åˆçš„æ‰©å±•æ–¹æ³• Select ç”Ÿæˆä¸€ä¸ªæ–°çš„é›†åˆï¼Œä¼ å…¥ Func å§”æ‰˜å¯¹è±¡
        var it2 = list.Select(new Func(delegate(C01Dog d) { return new C02SmallDog(); }));

        //7.4.2 ä½¿ç”¨é›†åˆçš„æ‰©å±•æ–¹æ³• Select ç”Ÿæˆä¸€ä¸ªæ–°çš„é›†åˆï¼Œç›´æ¥ä¼ å…¥ åŒ¿åæ–¹æ³•
        var it3 = list.Select(delegate(C01Dog d) { return new C02SmallDog(); });

        //7.4.3 æ¨¡æ‹Ÿä¸€ä¸ª Select æ–¹æ³•ï¼Œä¼ å…¥Funcå§”æ‰˜
        List list2 = list.MySelect(
            new Func(delegate(C01Dog d)
        {
            return new C02SmallDog() { Name = d.Name };
        })
        );

        //7.4.4 æ¨¡æ‹Ÿä¸€ä¸ªSelectæ–¹æ³•ï¼Œç›´æ¥ä¼ å…¥åŒ¿åæ–¹æ³•
        List listNew = list.MySelect(delegate(C01Dog d)
        {
            return new C02SmallDog() { Name = d.Name };
        });
    } 
    #endregion

    #region 7.4 è‡ªå·±æ¨¡æ‹Ÿä¸€ä¸ª Select æ–¹æ³•ï¼Œç”¨æ¥å°†ä¸€ä¸ªé›†åˆè½¬æˆå¦ä¸€ä¸ªé›†åˆ
    ///
    /// 7.4 è‡ªå·±æ¨¡æ‹Ÿä¸€ä¸ª Select æ–¹æ³•ï¼Œç”¨æ¥å°†ä¸€ä¸ªé›†åˆè½¬æˆå¦ä¸€ä¸ªé›†åˆ
    ///
    static ListMySelect(this Listlist, Funcfunc)
    {
        ListlistNew = new List();
        //éå†è€é›†åˆå…ƒç´ 
        foreach (T1 item in list)
        {
            //è°ƒç”¨ func å§”æ‰˜ å°† è€é›†åˆå…ƒç´  è½¬æˆ å¦ä¸€ä¸ª å…ƒç´  è¿”å›
            TR re = func(item);
            //å°†è½¬æ¢åçš„ æ–°å…ƒç´  ï¼Œå­˜å…¥æ–°é›†åˆ
            listNew.Add(re);
        }
        //è¿”å›æ–°é›†åˆ
        return listNew;
    } 
    #endregion

    #region 7.4* ç³»ç»Ÿæ³›å‹å§”æ‰˜ ç»“åˆ åŒ¿åç±»ä½¿ç”¨ - Func - è‡ªå®šä¹‰è¿”å›å€¼ç±»å‹
    ///
